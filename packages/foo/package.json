{
  "name": "@nighttrax/foo",
  "version": "1.0.0",
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn run clean && yarn run compile",
    "clean": "rimraf -rf ./dist tsconfig.build.tsbuildinfo",
    "compile": "tsc -b tsconfig.build.json",
    "compile:paths": "bob-ts -i src --paths true",
    "prepublishOnly": "yarn run build",
    "test": "yarn run build"
  },
  "devDependencies": {
    "rimraf": "~3.0.2",
    "typescript": "~4.5.2",
    "bob-ts": "^1.2.2",
    "bob-tsm": "^0.3.6"
  }
}
